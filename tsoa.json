{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controllers/*.ts", "src/model/*.ts"],
  "spec": {
    "outputDirectory": "./docs",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "./src"
  },
  "ignore": ["**/node_modules/**"]
}
