{
  "name": "guardian",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run tsoa:watch\" \"nodemon\"",
    "populate": "ts-node ./scripts/populate.ts",
    "build": "tsc",
    "lint": "eslint ./src --ext .ts",
    "tsoa": "npx tsoa spec-and-routes",
    "tsoa:watch": "nodemon --exec \"npm run tsoa\" --watch \"./src/controller/*.ts\" --watch \"./src/model/*.ts\" --watch \"./src/repository/*.ts\"",
    "test": "npm run db:init && npm run tsoa && jest --runInBand --coverage",
    "db:init": "mysql < ./setup/init.sql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pacokwon/guardian.git"
  },
  "author": "Paco Kwon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pacokwon/guardian/issues"
  },
  "homepage": "https://github.com/pacokwon/guardian#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/morgan": "^1.9.2",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^14.14.16",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "pretty-quick": "^3.1.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
